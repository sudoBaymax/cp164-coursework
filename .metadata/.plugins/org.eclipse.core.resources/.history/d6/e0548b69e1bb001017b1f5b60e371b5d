    def split_alt(self):
        """
        -------------------------------------------------------
        Splits the source list into separate target lists with values 
        alternating into the targets. At finish source self is empty.
        Order of source values is preserved.
        (iterative algorithm)
        Use: target1, target2 = source.split()
        -------------------------------------------------------
        Returns:
            target1 - contains alternating values from source (List)
            target2 - contains other alternating values from source (List)
        -------------------------------------------------------
        """
        # your code here
        return

    def split_alt_r(self):
        """
        -------------------------------------------------------
        Split a list into two parts. even contains the even indexed
        elements, odd contains the odd indexed elements.
        Order of even and odd is not significant. (recursive version)
        Use: even, odd = lst.split_alt_r()
        -------------------------------------------------------
        Returns:
            even - the even numbered elements of the list (List)
            odd - the odd numbered elements of the list (List)
        -------------------------------------------------------
        """
        even = List()
        odd = List()
    
        # call the separate helper method (helper will handle empty source)
        self._split_alt_r_aux(even, odd, True)   # start taking the even index (0)
        return even, odd
    
    
    def _split_alt_r_aux(self, even, odd, take_even):
        """
        Separate helper method (recursive). Moves one node per call from
        self to either even or odd. 'take_even' toggles each recursion.
        Parameters:
            even, odd - List objects to receive nodes
            take_even - bool: True -> move to even, False -> move to odd
        """
        # base case: source empty -> stop
        if self._front is None:
            return
    
        # detach the front node from self
        node = self._front
        self._front = node._next
        node._next = None
    
        # update source counters / rear
        self._count -= 1
        if self._front is None:
            self._rear = None
    
        # append node to destination list (even or odd)
        dst = even if take_even else odd
        if dst._front is None:
            dst._front = dst._rear = node
        else:
            dst._rear._next = node
            dst._rear = node
        dst._count += 1
    
        # recursive call (toggle which list receives the next node)
        self._split_alt_r_aux(even, odd, not take_even)

    def intersection(self, source1, source2):
        """
        -------------------------------------------------------
        Update the current list with values that appear in both
        source1 and source2. Values do not repeat. source1 and
        source2 are unchanged.
        (iterative algorithm)
        Use: target.intersection(source1, source2)
        -------------------------------------------------------
        Parameters:
            source1 - a linked list (List)
            source2 - a linked list (List)
        Returns:
            None
        -------------------------------------------------------
        """
        # your code here
        return

    def intersection_r(self, source1, source2):
        """
        -------------------------------------------------------
        Update the current list with values that appear in both
        source1 and source2. Values do not repeat. source1 and
        source2 are unchanged.
        (recursive algorithm)
        Use: target.intersection(source1, source2)
        -------------------------------------------------------
        Parameters:
            source1 - a linked list (List)
            source2 - a linked list (List)
        Returns:
            None
        -------------------------------------------------------
        """
        self._intersection_r_aux(source1._front, source2)
        return
    
    
    def _intersection_r_aux(self, node, source2):
        if node is None:
            result = None
        else:
            _, current_in_source2, _ = source2._linear_search(node._value)
            
            if current_in_source2 is not None:
                _, current_in_self, _ = self._linear_search(node._value)
                
                if current_in_self is None:
                    self.append(node._value)
            
            result = self._intersection_r_aux(node._next, source2)
        
        return result

    def union(self, source1, source2):
        """
        -------------------------------------------------------
        Update the current list with all values that appear in
        source1 and source2. Values do not repeat. source1 and
        source2 are unchanged.
        (iterative algorithm)
        Use: target.union(source1, source2)
        -------------------------------------------------------
        Parameters:
            source1 - a linked list (List)
            source2 - a linked list (List)
        Returns:
            None
        -------------------------------------------------------
        """
        # your code here
        return

    def union_r(self, source1, source2):
        """
        -------------------------------------------------------
        Update the current list with all values that appear in
        source1 and source2. Values do not repeat. source1 and
        source2 are unchanged.
        (recursive algorithm)
        Use: target.union(source1, source2)
        -------------------------------------------------------
        Parameters:
            source1 - a linked list (List)
            source2 - a linked list (List)
        Returns:
            None
        -------------------------------------------------------
        """
        self._union_r_aux(source1._front)
        self._union_r_aux(source2._front)
        return
    
    
    def _union_r_aux(self, node):
        if node is None:
            result = None
        else:
            _, current, _ = self._linear_search(node._value)
            
            if current is None:
                self.append(node._value)
            
            result = self._union_r_aux(node._next)
        
        return result

    def clean_r(self):
