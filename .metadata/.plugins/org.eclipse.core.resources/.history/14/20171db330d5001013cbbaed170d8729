# Imports

from BST_linked import BST

def bst_sum(bst):
    return bst_sum_aux(bst._root)
    
def bst_sum_aux(node):
    sum = 0
    
    if node is None:
        sum = 0
    else:
        sum = node._value + bst_sum_aux(node._left) + bst_sum_aux(node._right)

    return sum
    


